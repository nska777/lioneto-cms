{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
  "type": "uid",
  "targetField": "title",
  "required": true,
  "unique": true
},
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "brand": {
      "type": "enumeration",
      "enum": [
        "scandy",
        "amber",
        "salvador",
        "buongiorno",
        "elizabeth",
        "pitti"
      ]
    },
    "cat": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "fasadi",
        "komody",
        "krovati",
        "plintusy",
        "polki",
        "shkafy",
        "stellaji",
        "stoli",
        "tumby",
        "veshalki",
        "vitrini",
        "zerkala",
        "pufi",
        "bedrooms",
        "living",
        "youth"
      ]
    },
    "collection": {
      "type": "string"
    },
    "module": {
      "type": "string"
    },
    "media": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "priceUZS": {
      "type": "integer"
    },
    "priceRUB": {
      "type": "integer"
    },
    "oldPriceUZS": {
      "type": "integer"
    },
    "oldPriceRUB": {
      "type": "integer"
    },
    "sortOrder": {
      "type": "integer"
    },
    "collectionBadge": {
      "type": "enumeration",
      "enum": [
        "Хит продаж",
        "Лучшая цена",
        "Супер акция",
        "Распродажа",
        "Только сегодня ",
        "Успейте купить"
      ]
    },
    "variants": {
      "type": "component",
      "component": "product.variant",
      "repeatable": true
    }
  }
}
